<Window 
        xmlns                 ='http://schemas.microsoft.com/netfx/2007/xaml/presentation' 
        xmlns:x               ='http://schemas.microsoft.com/winfx/2006/xaml' 
        x:Class               ="SoftTeach.View.Stundenentwürfe.StundenentwurfFixedDocument"
        Title                 ='Stundenentwurf Ausdruck' 
        Height                ='600' 
        Width                 ='800' 
        WindowStartupLocation ='CenterOwner'>
  <DocumentViewer Name='documentViewer' Style="{StaticResource SoftTeachDocumentViewer}" HorizontalPageSpacing="25" VerticalPageSpacing="25">
    <DocumentViewer.CommandBindings>
      <CommandBinding Command="ApplicationCommands.Copy" CanExecute="CommandBindingCanExecuteCopy"  />
    </DocumentViewer.CommandBindings>
  </DocumentViewer>
</Window>